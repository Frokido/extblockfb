{
  "manifest_version": 3,
  "name": "Facebook Content Filter",
  "version": "1.0",
  "description": "A browser extension to filter out unwanted content from your Facebook feed.",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}